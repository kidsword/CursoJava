package desafios;

public class DesafioLogicos {
	public static void main(String[] args) {

		var trabalho1 = false;
		var trabalho2 = false;
		
		var comprouTv50 = trabalho1 && trabalho2;
		var comprouTv32 = trabalho1 ^ trabalho2; 		
		var tomouSorvete = trabalho1 || trabalho2;
		
		if(comprouTv32) 
			System.out.print("Compramos televis達o de 32 polegadas");
		else if (comprouTv50)
			System.out.print("Compramos televis達o de 50 polegadas");
		else
			System.out.print("N達o Compramos nenhuma televis達o");
		
		if(tomouSorvete)
			System.out.print(" e tomamos sorvete!");
		else
			System.out.print(" e ficamos com fome... :(");
		
		
	}	
	

}
