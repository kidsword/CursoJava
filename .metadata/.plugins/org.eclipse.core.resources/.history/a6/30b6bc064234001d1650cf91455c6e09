package fundamentos;

public class Temperatura {
	public static void main(String[] args) {
		final double FahrenheitZero = 32.0;
		final double FahrenheitDiv= 5.0/9.0;
		double temperF = 10.0;
		double result = (temperF - FahrenheitZero) * FahrenheitDiv;
		
		System.out.println(result);
		
		
	}

}
